{
	"buildCommand": "pnpm build:vercel",
	"installCommand": "pnpm install --no-frozen-lockfile",
	"framework": null,
	"devCommand": "pnpm dev",
	"outputDirectory": "dist",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*).json",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600, must-revalidate"
				}
			]
		}
	]
}
